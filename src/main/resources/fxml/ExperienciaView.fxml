<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="view" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="experienciaTable" editable="true" prefHeight="200.0" prefWidth="550.0" HBox.hgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="desdeColumn" prefWidth="100.0" text="Desde" />
            <TableColumn fx:id="hastaColumn" prefWidth="100.0" text="Hasta" />
            <TableColumn fx:id="denominacionColumn" prefWidth="100.0" text="Denominación" />
            <TableColumn fx:id="empleadorColumn" prefWidth="100.0" text="Empleador" />
         </columns>
      </TableView>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Button fx:id="aniadirButton" mnemonicParsing="false" onAction="#onAniadirAction" prefWidth="60.0" text="Añadir">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="eliminarButton" mnemonicParsing="false" onAction="#onEliminarAction" prefWidth="60.0" text="Eliminar" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
